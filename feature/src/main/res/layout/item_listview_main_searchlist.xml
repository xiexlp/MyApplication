<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="vertical"
        >



        <TextView
            android:id="@+id/doc_id_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="doc id"
            />

        <TextView
            android:id="@+id/doc_url_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="doc url"
            />

        <TextView
            android:id="@+id/topic_title_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="topic title"
            />

        <TextView
            android:id="@+id/topic_score_text_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="得分"
            />
    </LinearLayout>





    <TextView
        android:id="@+id/topic_content_text_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="sectionName" />

    <!--<TextView-->
        <!--android:id="@+id/text_item_listview_username"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:text="sectionName" />-->






</LinearLayout>